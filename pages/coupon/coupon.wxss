/* pages/coupon/coupon.wxss */
/* 外层盒子 */
.content{
  width: 100%;
  min-height: 1216rpx;
  height: auto;
  background-color: #F5F5F5;
  box-sizing: border-box;
  padding: 60rpx 30rpx;
}
/* 优惠券项 */
.coupon{
  width:100%;
  height:200rpx;
  position: relative;
}
/* 折扣力度字体样式部分 */
.discount{
  height: 200rpx;
  width: 200rpx;
  position: absolute;
  left: 0rpx;
  top: 0rpx;
  background: red;
}