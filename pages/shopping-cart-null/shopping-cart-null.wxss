/* pages/shopping-cart-null/shopping-cart-null.wxss */
/* 最外层盒子 */
.content{
  height: 1120rpx;
  width: 100%;
  box-sizing: border-box;
  padding-top: 162rpx;
}
/* 购物车为空的图片*/
.Shopping-cart-icon{
  width:100%;
  height:248rpx;
  text-align: center;
  /* padding: 162rpx 243rpx 0 218rpx; */
  box-sizing: border-box;
  margin-bottom: 55rpx;
}
/* 购物车提示语 */
.cue-words{
  width: 100%;
  height: 23rpx;
  text-align: center;
  line-height: 23rpx;
  font-size:24rpx;
  font-weight:bold;
  color:rgba(153,153,153,1);
  margin-bottom: 26rpx;
}
/* 立即购买链接 */
.buy-button{
  width: 100%;
  height: 55rpx;
  font-size:24rpx;
  font-weight:bold;
  text-decoration:underline;
  text-align: center;
  color:rgba(9,8,8,1);
}
